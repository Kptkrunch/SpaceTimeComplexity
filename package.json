{
  "name": "space-time-complexity",
  "version": "1.0.0",
  "description": "Exercises to help with understanding of Big O notation",
  "main": "index.js",
  "scripts": {
    "jest": "jest --coverage --watchAll",
    "test": "npm jest -- -u",
    "build": "webpack -w --config webpack.config.js --mode production",
    "start": "webpack-dev-server --open",
    "server": "nodemon server/server.js"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
