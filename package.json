      "./src/setupTests.js"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kptkrunch/SpaceTimeComplexity"
  },
